<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
  ui:generateFormat="com.google.gwt.i18n.rebind.format.PropertiesFormat"
  ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
  ui:generateLocales="default,de"
  xmlns:g="urn:import:com.google.gwt.user.client.ui">

  <ui:style src="SecretViewGwtImpl.css"/>

  <g:HTMLPanel styleName="{style.centerPanel}">
   <g:HTML styleName="{style.header}">
     <h1><ui:msg description="header">Secret Dummy Page</ui:msg></h1>
   </g:HTML>
   <g:HTML styleName="{style.output}">
     <h1><ui:msg description="text">This Page is only here to demonstrate the protection of a page from unauthorized users.</ui:msg></h1>
   </g:HTML>
  </g:HTMLPanel>
</ui:UiBinder>
